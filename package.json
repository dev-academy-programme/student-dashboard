{
  "name": "i-visualise-commits",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "run-p dev:*",
    "dev:server": "tsx watch src/index.ts",
    "dev:css": "tailwind -i ./src/styles/input.css -o ./public/output.css --watch",
    "build": "run-s build:*",
    "build:astro": "tsc src/index.tsx --outDir dist",
    "build:prisma": "prisma generate",
    "build:css": "tailwind -i ./src/styles/input.css -o ./public/output.css",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "delete": "ts-node src/script.ts",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.19",
    "@types/react-dom": "^18.2.7",
    "dotenv": "^16.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "prisma": "^4.16.2",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/rss": "^2.4.3",
    "@astrojs/sitemap": "^2.0.1",
    "@astrojs/tailwind": "^4.0.0",
    "@astrojs/ts-plugin": "^1.1.1",
    "@prisma/client": "4.16.2",
    "astro": "^2.9.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
